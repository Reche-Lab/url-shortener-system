{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/url-shortener-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/url-shortener-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/url-shortener-system",
  "projects": {
    "identity-service": {
      "type": "application",
      "root": "apps/identity-service",
      "entryFile": "main",
      "sourceRoot": "apps/identity-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/identity-service/tsconfig.app.json"
      }
    },
    "url-shortener-service": {
      "type": "application",
      "root": "apps/url-shortener-service",
      "entryFile": "main",
      "sourceRoot": "apps/url-shortener-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/url-shortener-service/tsconfig.app.json"
      }
    },
    "url-shortener-system": {
      "type": "application",
      "root": "apps/url-shortener-system",
      "entryFile": "main",
      "sourceRoot": "apps/url-shortener-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/url-shortener-system/tsconfig.app.json"
      }
    }
  }
}